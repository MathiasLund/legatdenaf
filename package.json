{
  "name": "legatdenaf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node --presets es2015,react server.js",
    "build": "npm run compile-client && npm run compile-server",
    "compile-client": "NODE_ENV=production webpack --progress --config webpack.config.js",
    "compile-server": "NODE_ENV=production webpack --progress --config webpack.server.config.js",
    "watch": "webpack -w --config webpack.config.js",
    "dev": "nodemon server.js --exec babel-node --presets es2015,react"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "font-awesome": "^4.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "q": "^1.4.1",
    "querystring": "^0.2.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.5.4",
    "react-router": "^2.8.1",
    "request": "^2.74.0",
    "request-promise": "^4.1.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "async": "^2.1.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "browser-refresh": "^1.6.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "postcss-color-function": "^2.0.1",
    "postcss-loader": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  }
}
